{% if(it.type == 'addpic') { %}
<img src="{%=it.url%}" onload="window.compressImage(this);">
<div class="delete-btn">
    <i class="aui-iconfont aui-icon-trash"></i>
</div>
{% }else if(it.type == 'delpic') { %}
<div class="aui-border aui-text-center add-btn">
    <i class="aui-iconfont aui-icon-plus"></i>
</div>
{% } %}